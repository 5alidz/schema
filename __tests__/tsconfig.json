{
  "compileOnSave": false,
  "compilerOptions": {
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
